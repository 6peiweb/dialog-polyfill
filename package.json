{
  "name": "dialog-polyfill",
  "version": "0.4.10",
  "description": "Polyfill for the dialog element",
  "main": "dialog-polyfill.cjs.js",
  "module": "dialog-polyfill.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleChrome/dialog-polyfill.git"
  },
  "author": "The Chromium Authors",
  "license": "BSD",
  "homepage": "https://github.com/GoogleChrome/dialog-polyfill",
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.1.2"
  },
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:cjs": "rollup index.js --file dialog-polyfill.cjs.js --format cjs",
    "build:esm": "rollup index.js --file dialog-polyfill.esm.js --format esm",
    "build:global": "rollup index.js --file dialog-polyfill.js --format iife --name dialogPolyfill",
    "prepublishOnly": "npm run build",
    "test": "tap test/"
  }
}
